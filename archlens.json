{
  "$schema": "https://raw.githubusercontent.com/archlens/ArchLens/master/config.schema.json",
  "name": "archlens_test",
  "rootFolder": "src",
  "github": {
    "url": "https://github.com/mathiasbk1/archtest",
    "branch": "main"
  },
  "saveLocation": "./diagrams/",
  "views": {
    "completeView": {
      "packages": [
        
      ],
      "ignorePackages": []
    },
    "apiView": {
      "packages": [
        {
          "path": "./*",
          "depth": 0
        }
      ],
      "ignorePackages": []
    },
    "apiViewDepth": {
      "packages": [
        {
          "path": "api",
          "depth": 1
        }
      ],
      "ignorePackages": []
    }
  }
}
